<!DOCTYPE html>
<html>
<head>
  <title>Music</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if current_user.nil?  %>
<%= link_to "Sign in", new_session_url %>
<%= link_to "Sign up", new_user_url %>
<% else  %>
<%= current_user.email %>
<%= button_to "Logout",session_url, method: :delete %>
<% end %>

<%= yield %>

</body>
</html>
